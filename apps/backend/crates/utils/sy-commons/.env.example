# Symphony Environment Variables Example
# Copy this file to .env to set environment variables for development
# These variables override TOML configuration settings

# Environment selection (development, production, test)
SYMPHONY_ENV=development

# Logging configuration overrides
# SYMPHONY_LOGGING_LEVEL=debug
# SYMPHONY_LOGGING_CONSOLE_ENABLED=true
# SYMPHONY_LOGGING_CONSOLE_FORMAT=pretty
# SYMPHONY_LOGGING_CONSOLE_COLORS=true

# File logging overrides
# SYMPHONY_LOGGING_FILE_ENABLED=true
# SYMPHONY_LOGGING_FILE_PATH=logs/custom.log
# SYMPHONY_LOGGING_FILE_ROTATION=daily
# SYMPHONY_LOGGING_FILE_MAX_FILES=7

# JSON logging overrides
# SYMPHONY_LOGGING_JSON_ENABLED=false
# SYMPHONY_LOGGING_JSON_PATH=logs/structured.json
# SYMPHONY_LOGGING_JSON_INCLUDE_SPANS=false

# Filesystem overrides
# SYMPHONY_FILESYSTEM_TEMP_DIR=tmp/symphony
# SYMPHONY_FILESYSTEM_MAX_FILE_SIZE=104857600
# SYMPHONY_FILESYSTEM_ATOMIC_WRITES=true

# Debug overrides
# SYMPHONY_DEBUG_DUCK_DEBUGGING=true
# SYMPHONY_DEBUG_PERFORMANCE_LOGGING=false